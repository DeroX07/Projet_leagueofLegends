@model IEnumerable<Projet_LeagueofLegends.Models.Equipe>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table">
    <tr>
        <th>
            Equipes
        </th>
        <th>
            Victoires
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TeamName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nbrematchgagné)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EquipeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.EquipeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EquipeID })
        </td>
    </tr>
}

</table>
